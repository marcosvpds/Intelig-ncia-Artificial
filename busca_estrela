#include <stdlib.h>
#include <stdio.h>
#include <math.h>

typedef struct Estado {
    int Valor[3][3]; // dados das posições
    int custo; // valor do custo total do estado
    int percorrido; // armazena se o estado já foi percorrido
};

typedef struct Estado Estado;

int h (Estado Entrada, Estado Final) {
    int h = 0;
    int i,j;
    for(i = 0; i < 3; i++){
        for(j = 0; j < 3; j++) {
            h += abs()
        }
    }
}

int main() {
    Estado *Inicial = malloc(sizeof(Estado));
    Inicial.custo = h(Inicial);
    Inicial.percorrido = 0;
    printf("%d", Inicial.custo);
    return 0;
}

